<?xml version="1.0" encoding="utf-8"?>
<biz.advancedcalendar.views.CheckableLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent"
	android:layout_height="48dp"
	android:baselineAligned="false"
	android:focusable="false" >
	<LinearLayout
		android:id="@+id/treeview_list_item_image_layout"
		android:layout_width="48dp"
		android:layout_height="fill_parent"
		android:focusable="false"
		android:gravity="right|center_vertical" >
		<ImageView
			android:id="@+id/treeview_list_item_image"
			android:layout_width="fill_parent"
			android:layout_height="fill_parent"
			android:focusable="false"
			android:src="@drawable/collapsed"
			android:visibility="gone" >
		</ImageView>
	</LinearLayout>
	<biz.advancedcalendar.views.CheckableFrameLayout
		android:id="@+id/treeview_list_item_frame"
		android:layout_width="fill_parent"
		android:layout_height="fill_parent"
		android:layout_weight="1"
		android:focusable="false" >
	</biz.advancedcalendar.views.CheckableFrameLayout>
</biz.advancedcalendar.views.CheckableLinearLayout>