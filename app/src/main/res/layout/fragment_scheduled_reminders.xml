<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/fragment_scheduled_reminders_linearlayout"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical" >
	<TextView
		android:id="@+id/fragment_scheduled_reminders_textview_warning_alarms_are_silenced_until_datetime"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:background="@color/Aqua"
		android:textAppearance="?android:attr/textAppearanceMedium"
		android:visibility="gone" />
	<ExpandableListView
		android:id="@+id/fragment_scheduled_reminders_expandable_list_view"
		android:layout_width="match_parent"
		android:layout_height="match_parent" />
</LinearLayout>